package testing;

import javax.swing.*;


import java.awt.event.*;
import java.awt.*;

@SuppressWarnings("serial")
public class aaa extends JFrame{
	private Ball b;

	public aaa() {

		final Ball b = new Ball();
		add(b, BorderLayout.CENTER);
	}

	public static void main(String[] args) {

		aaa moveBall = new aaa();
		moveBall.setTitle("Use Keys to Move The Ball");
		moveBall.setSize(815, 500);
		moveBall.setLocationRelativeTo(null);
		moveBall.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		moveBall.setVisible(true);
		moveBall.setBackground(Color.BLUE);
		
	}

}
